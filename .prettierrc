{
  "printWidth": 100,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "importOrder": [
    "^express$",
    "^express/(.*)$",
    "^express-(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^[./]"
  ]
 }
